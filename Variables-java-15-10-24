class HelloWorld {
    public static void main(String[] args) {
        
        //Enteras
        int entera;
        System.out.println("El rando de un entero es negativo 2,147,483,648 a 2,147,483,648");
        Integer entero;
        
        long enterogoGrande;
        System.out.println("El rando de un long es negativo 9,223,372,036,854,775,808 a 9,223,372,036,854,775,807");
        Long enterogoGrandeObject;
        
        short enteroPequeno;
        System.out.println("El rando de un short es 32.768 negativo a 127 positivo.");
        Short enteroPequenoObject;
        
        byte enteroMuyPequeno;
        System.out.println("El rando de un byte es 128 negativo a 127 positivo");
        Byte enteroMuyPequenoObject;
        
        //Bolean
        boolean bool;
        System.out.println("El rando de un boolean solo es Verdadero - Falso");
        Boolean booleanObject;
        
        
        //Decimal 
        float decimal;
        System.out.println("El rando de un float es 1.4 negativo 045 a 3.4e positivo 038");
        Float decimalObject;
        
        
        double decimal2;
        System.out.println("El rando de un double es 4.9e negativo 324 to 1.8e positivo 308");
        Double decimal2Object;
        
        
        
        //cadena de caracteres 
        String cadena; 
        System.out.println("El string puede imprimir cadenas de palabras");
        
        //caracter
        char caracter;
        System.out.println("El char puede tener un simbolo, una letra o una valor numerico que represente una letra");
    }
}
